		"@sveltejs/kit": "next",
		"@types/lodash": "^4.14.182",
		"@types/lodash-es": "^4.17.6",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"carbon-components-svelte": "^0.67.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
